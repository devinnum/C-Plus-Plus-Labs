#include <iostream>
#include <fstream>
#include <cstdlib>
#include <string>
#include <map>
#include <unordered_set>

using namespace std;

int main(int argc, char** argv) {
  if (argc != 2) {
    cout << "Must supply the input file name as the one and only parameter" << endl;
    exit(1);
  }

  ifstream file(argv[1], ifstream::binary);
  if (!file.is_open()) {
    cout << "Unable to open file '" << argv[1] << "'." << endl;
    exit(2);
  }

