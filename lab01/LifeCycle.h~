#ifndef LIFECYCLE_H
#define LiFECYCLE_H

class MyObject {
 public:
  static int numObjs;

  MyObject();
  MyObject(string n);
  MyOject(const MyObject& rhs);
  ~MyObject();

  string getName() const;
  void setName(const string& newName);

  friend ostream& operator<<(ostream& output, const MyObject& obj);
 private:
  string name;
  int id;
};
  
